function myFunction() {
  
  var groupList = AdminDirectory.Groups.list( {
  
    customer : "my_customer"
                 
  }
       )                                      
  
  var groupArray = [];
  
                                             
  
  for (var i=0;i<groupList.groups.length;i++) {
    
  groupArray[i] = groupList.groups[i].email;
  
  //Logger.log(groupArray[i]);
    
   SpreadsheetApp.getActiveSheet().getRange("A"+(i+1)).setValue(groupArray[i]);
  
  }
                                             
  
}
