function changePrimaryAndAddAlias() {
    
  var maxRows = SpreadsheetApp.getActiveSpreadsheet().getActiveSheet().getLastRow();
  var groups = SpreadsheetApp.getActiveSpreadsheet().getActiveSheet().getRange("A1:A"+maxRows).getValues(); //Add group emails to column A of the spreadsheet
  
  
  for (var i=0;i<groups.length;i++)
  {
    var userName = groups[i];
    AdminDirectory.Groups.update({"email": userName + "@c.gameofclouds.com"}, userName + "@b.gameofclouds.com"); //This is taking a group that is on @c.gameofclouds.com and changing it to @b.gameofclouds.com. Please edit the domain names to your own subdomains.
  }
  }
